= form_for @about do |f|
  - if @about.errors.any?
    #error_explanation
      %h2= "#{pluralize(@about.errors.count, "error")} prohibited this about from being saved:"
      %ul
        - @about.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :history
    = f.text_area :history
  .field
    = f.label :commitment
    = f.text_area :commitment
  .field
    = f.label :vision
    = f.text_area :vision
  .field
    = f.label :mission
    = f.text_area :mission
  .actions
    = f.submit 'Save'
